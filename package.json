{
  "name": "monke-task",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "main": "src/index.js",
  "repository": "https://github.com/stelianok/monke-task.git",
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "heroku"
  ],
  "author": "Kaua Steliano <stelianok@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "deploy-command": "node src/deploy-command.js",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@types/express": "^4.17.13",
    "axios": "^0.21.4",
    "discord-api-types": "^0.22.0",
    "discord.js": "^14.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  }
}
